import java.util.*;

public class FirstClass{                //HashMap
    public static void main(String[] args){
        HashMap<String, Integer> map = new HashMap<>();
        map.put("India", 120);
        map.put("China", 130);
        map.put("Malaysia", 140);
        map.put("Africa", 150);
        map.put("Antartica", 160);

        System.out.println(map);
    }
}






public class FirstClass{                    //Duplicacy HashMap
    public static void main(String[] args){
        HashMap<String, Integer> map = new HashMap<>();
        map.put("India", 120);
        map.put("China", 130);
        map.put("Malaysia", 140);
        map.put("Africa", 150);
        map.put("Antartica", 160);

        System.out.println(map);
        map.put("India", 180);
        System.out.println(map);
    }
}











public class FirstClass{            //Sercahing HashMap
    public static void main(String[] args){
        HashMap<String, Integer> map = new HashMap<>();
        map.put("India", 120);
        map.put("China", 130);
        map.put("Malaysia", 140);
        map.put("Africa", 150);
        map.put("Antartica", 160);

        System.out.println(map);
        map.put("India", 180);
        System.out.println(map);

        if(map.containsKey("China")){
            System.out.println("Key is present");
        }
        else{
            System.out.println("Key is not present");
        }

        System.out.println(map.get("China"));
        System.out.println(map.get("Kerela"));
    }









